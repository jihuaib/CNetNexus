<?xml version="1.0" encoding="UTF-8"?>
<configuration module_id="1">
    <!-- Command group definitions -->
    <command_groups>
        <!-- Basic command group -->
        <group name="nn_cfg_show">
            <elements>
                <element id="1" type="keyword">
                    <name>show</name>
                    <description>Display system information</description>
                </element>
                <element id="2" type="keyword">
                    <name>cli</name>
                    <description>CLI information</description>
                </element>
                <element id="3" type="keyword">
                    <name>tree</name>
                    <description>Command tree structure</description>
                </element>
            </elements>

            <commands>
                <command>
                    <expression>1 2 3</expression>
                    <views>global</views>
                    <callback>cmd_show_tree</callback>
                </command>
            </commands>
        </group>

        <group name="nn_cfg_config">
            <elements>
                <element id="1" type="keyword">
                    <name>quit</name>
                    <description>Exit current mode</description>
                </element>
                <element id="2" type="keyword">
                    <name>configure</name>
                    <description>Enter configuration mode</description>
                </element>
            </elements>

            <commands>
                <command>
                    <expression>1</expression>
                    <views>global</views>
                    <callback>cmd_exit</callback>
                </command>

                <command>
                    <expression>2</expression>
                    <views>user</views>
                    <callback>cmd_configure</callback>
                </command>
            </commands>
        </group>
    </command_groups>
</configuration>
